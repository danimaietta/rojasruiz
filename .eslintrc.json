{
  "extends": "next/core-web-vitals",
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "parserOptions": {
      "ecmaVersion": 12
  },
  "rules": {
      "indent": [
          "error"
      ],
      "linebreak-style": [
          "error",
          "unix"
      ],
      "quotes": [
          "error",
          "single"
      ],
      "semi": [
          "error",
          "never"
      ]
  },
  "order/properties-alphabetical-order": true
}
